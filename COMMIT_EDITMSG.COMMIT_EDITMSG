+#include "Torneig.hh"
+//a
+#include <vector>
+#include <iostream>
+#include <string>
+
+Torneig::Torneig(string nom_torneig, Categoria categoria_torneig)
+{
+    this->categoria = categoria_torneig;
+    this->nom = nom_torneig;
+}
+
+void Torneig::construir_arbre(BinTree<int> &a, int i, int k, int k_max)
+{
+    // cas base k - i > k_max
+
+    if (k - i + 1 > k_max) {
+        a = BinTree<int>(i);
+        cout << a.value() << "."  << ranking[a.value() - 1];
+        return;
+    }
+
+    BinTree<int> left = BinTree<int>();
+    BinTree<int> right = BinTree<int>();
+
+    cout << "(";
+    construir_arbre(left, i, k * 2, k_max);
+    cout << " ";
+    construir_arbre(right, k - i + 1, k * 2, k_max);
+    cout << ")";
+
+    a = BinTree<int>(i, left, right);
+}
+
+void Torneig::escriure_arbre(const BinTree<int> &a)
+{
+    if (a.left().empty() and a.right().empty()) {
+        cout << a.value() << "." << ranking[a.value() - 1];
+        return;
+    }
+
+    cout << "(";
+    escriure_arbre(a.left());
+    cout << " ";
+    escriure_arbre(a.right());
+    cout << ")";
+}
+
+void Torneig::iniciar_torneig(const vector<pair<string, int> > &ranking)
+{
+    /* entren un nombre n */
+    int n;
+    cin >> n;
+
+    this->ranking = vector<string>(n);
+
+    for (int i = 0; i < n; ++i) {
+        int ordre;
+        cin >> ordre;
+        this->ranking[i] = ranking[ordre - 1].first;
+    }
+
+    construir_arbre(quadre_emparellaments, 1, 2, n);
+    // escriure_arbre(quadre_emparellaments);
+    cout << endl;
+}
+
+void Torneig::finalitzar_torneig() {
+        
+}
+
+Classificacio Torneig::consultar_darrera_classificacio() const
+{
+    return this->darrera_classificacio;
+}
+
+string Torneig::consultar_nom_categoria() const
+{
+    return this->categoria.nom;
+}
diff --git a/Torneig.hh b/Torneig.hh
new file mode 100755
index 0000000..bcca4aa
--- /dev/null
+++ b/Torneig.hh
@@ -0,0 +1,90 @@
+/** @file Torneig.hh
+ * @brief Especificació de la classe Torneig.
+ */
+
+#ifndef _TORNEIG_HH_
+#define _TORNEIG_HH_
+
+#include "Categoria.hh"
+#include "Classificacio.hh"
+#include "BinTree.hh"
+
+#ifndef NO_DIAGRAM 
+#include "Resultat.hh"
+
+#include <iostream>
+#include <string>
+#endif
+
+/** @class Torneig
+ * @brief Representa un torneig del circuit.
+ * 
+ * Aquest consta de dues funcions
+ * principals, la d'inici i la de final. La de final resumeix la tasca del
+ * torneig, retornar un classificació que resumeixi que ha passar.
+ * A més, un torneig sempre guarda la classificació de la última edició, perque 
+ * farà falta treure punts a certs jugadors, ja sigui quan es borrir el torneig
+ * o s'en faci una nova edició.
+ */
+class Torneig {
+
+	public:
+
+		/** @brief Constructora principal: Construeix un Torneig en funcio
+		 * d'un nom i una categoria. La categoria s'emmagatzema per a saber
+		 * quants punts corresponen a cada jugador en acabar el torneig.
+		 * \pre Un string (nom del torneig) i una categoria.
+		 * \post Es crea un torneig amb el nom i la categoria corresponents.
+		 */
+		Torneig(string nom_torneig, Categoria categoria_torneig);
+
+		/** @brief Iniciar Torneig: Inicia el torneig, es a dir, llegeix les 
+		 * inscripcions, es confecciona i imprimeix l'arbre d'emparellaments.
+		 * \pre El ranking en el moment de començar el torneig.
+		 * El torneig no està començat (cert), i a l'entrada es troba un 
+		 * enter n, seguit de n strings (noms de jugadors) i n enters (posicions ranking).
+		 * \post Es llegeixen les inscripcions i es confecciona i imprimeix el
+		 * quadre d'emparellaments
+		 */
+		void iniciar_torneig(const vector<pair<string, int> > &ranking);
+
+		/** @brief Finalitzar Torneig: Es llegeixen els resultats del torneix 
+		 * i es retorna una classificacio, que indica quants punts ha guanyat
+		 * cada jugador i quines estadísitques ha sumat. Si un jugador va 
+		 * participar en la pasada edició ja se li han restat aquells punts. 
+		 * A més, el torneig guarda una copia de la classificació, que servirà
+		 * per a la seguent edició o per a quan s'elimini el torneig.
+		 * \pre El torneig s'ha iniciat.
+		 * \post Es llegeixen els resultats del torneig i es retorna i guarda
+		 * la classificació.
+		 */
+		Classificacio finalitzar_torneig();
+
+		/** @brief Consultar Darrera Classifiacio: Retorna la classificaió de la
+		 * útlima edició del torneig.
+		 * \pre Cert.
+		 * \post Una copia de la darrera classificacio.
+		 */
+		Classificacio consultar_darrera_classificacio() const;
+		
+		/** @brief Consultar Nom Categoria: Retorna un string amb el nom de la
+		 * categoria.
+		 * \pre Cert.
+		 * \post Nom de la categoria.
+		 */
+		string consultar_nom_categoria() const;
+
+	private:
+        
+        void construir_arbre(BinTree<int> &a, int i, int k, int k_max);
+        void escriure_arbre(const BinTree<int> &a);
+
+		string nom;
+		vector<string> ranking;
+		BinTree<int> quadre_emparellaments;
+		Categoria categoria;
+		Classificacio darrera_classificacio;
+
+};
+
+#endif
